<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" >

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light" tools:ignore="NewApi" >
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:actionOverflowButtonStyle">@style/ActionButtonOverflow</item>
        <item name="actionOverflowButtonStyle">@style/ActionButtonOverflow</item>
    </style>

    <style name="ActionButtonOverflow" parent="Base.Widget.AppCompat.ActionButton.Overflow">
        <item name="android:src">@drawable/more_info_page_1</item>
    </style>

    <style name="BaseTheme" parent="Theme.AppCompat" tools:ignore="NewApi">
        <item name="android:actionOverflowButtonStyle">@style/ActionButtonOverflow</item>
        <item name="actionOverflowButtonStyle">@style/ActionButtonOverflow</item>
        <item name="android:actionBarStyle" tools:ignore="NewApi">@style/TransparentActionBarTheme</item>
        <item name="actionBarStyle">@style/LightActionBar</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- the theme applied to the application or activity -->
    <style name="TransparentBar" parent="Theme.AppCompat" tools:ignore="NewApi">
